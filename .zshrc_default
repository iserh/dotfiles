# -------------------- oh-my-zsh installation --------------------

# Path to your oh-my-zsh installation.
export ZSH="$HOME/.oh-my-zsh"

ZSH_THEME="robbyrussell"
plugins=(git tmux conda-zsh-completion)

source $ZSH/oh-my-zsh.sh

# load dotfiles customization file
ZSH_CUSTOMIZATION=$HOME/dotfiles/zsh_customization
if [ -f "$ZSH_CUSTOMIZATION" ]; then
    source $ZSH_CUSTOMIZATION
fi


# -------------------- USER CUSTOMIZATION ------------------------


